menuItem::menuItem(const char* tag) : TAG(tag) {};

menuItem::~menuItem(){


}

void menuItem::print(){
	printf("%s\r\n", this->TAG);
}




radioMenus::radioMenus() {
	this->audioDevicesCounter=0;
	this->perypheriaDevicesCounter=0;
	this->audioDevices=NULL;
	this->perypheriaDevices=NULL;

	printf("%sMain menu object had been created.\r\n", this->TAG);
}

