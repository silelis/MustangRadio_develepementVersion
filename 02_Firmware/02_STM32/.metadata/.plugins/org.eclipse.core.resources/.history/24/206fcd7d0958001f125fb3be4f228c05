#include "SileliS_code/radioMenu.h"


menuItem::menuItem(const char* tag) : TAG(tag) {
	printf("%s: Emptu menu had been created. Please set it up.\r\n", this->TAG);
};

void menuItem::menuName(void){
	printf("%s\r\n"
			"n",this->TAG);
}

menuItem::~menuItem(){
}




radioMenus::radioMenus() {

	printf("%sMain menu object had been created.\r\n", this->TAG);
	printf("%d\r\n", this->audioDev.size());

}



void radioMenus::emplace_back(){
	this->audioDev.emplace_back("si468x_DAB+");
	printf("%d\r\n", this->audioDev.size());

}


