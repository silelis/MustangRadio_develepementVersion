#include "SileliS_code/menuItem.h"



menuItem::menuItem(const char* tag, uint8_t execFunctionArraySize) {
	this->TAG = tag;
	printf("$s: menuItem had been created. Please append functiond.\r\n", this->TAG);
	this->createExecFunctions(execFunctionArraySize);

};

void menuItem::deleteExecFunctions(void){
	if (this->pExecFunction!=null)
		delete [] pExecFunction;
}

bool menuItem::createExecFunctions(uint8_t arraySize){
	pExecFunction = nullptr;
}

menuItem::~menuItem(){
	this->deleteExecFunctions();
}

